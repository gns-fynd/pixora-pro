# API settings
API_V1_STR=/api
PROJECT_NAME=Pixora AI

# Security
SUPABASE_URL=your-supabase-project-url
SUPABASE_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-role-key
SUPABASE_JWT_SECRET=your-supabase-jwt-secret

# Storage Configuration
STORAGE_TYPE=supabase
STORAGE_BUCKET=pixora
STORAGE_PUBLIC_URL=

# AI Services
OPENAI_API_KEY=your-openai-api-key
REPLICATE_API_TOKEN=your-replicate-api-token
FAL_CLIENT_API_KEY=your-fal-ai-api-key

# Fal.ai Model Endpoints
FAL_TEXT_TO_IMAGE_MODEL=fal-ai/flux/dev
FAL_IMAGE_TO_VIDEO_MODEL=fal-ai/kling-video/v1.6/pro/image-to-video
FAL_TEXT_TO_SPEECH_MODEL=fal-ai/elevenlabs/tts/multilingual-v2
FAL_TEXT_TO_MUSIC_MODEL=fal-ai/elevenlabs/sound-effects

# JWT Configuration
JWT_SECRET=your-jwt-secret
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=60

# Server
HOST=0.0.0.0
PORT=8000
RELOAD=True

# Logging Configuration
LOG_LEVEL=info
ENABLE_JSON_LOGS=false
LOG_FILE=
